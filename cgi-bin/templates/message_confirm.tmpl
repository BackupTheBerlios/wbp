<body>

<TMPL_INCLUDE header.tmpl> 

<h1>Messages - Confirmation</h1>


<form action="<TMPL_VAR FORM>" method="post">

<input type="hidden" name="action" value="<TMPL_VAR ACTION>">
<input type="hidden" name="sid" value="<TMPL_VAR SID>">
<input type="hidden" name="method" value="<TMPL_VAR METHOD>">

<table cellspacing="0" cellpadding="0" border="0" align="center">
<tr>
	<td colspan="2" align="left">
	<TMPL_IF MSG>
		<font color="#ff0000"><TMPL_VAR MSG></font>
	</TMPL_IF>
	</td>
</tr>
<tr>
	<td>
	<b>To:</b>
	</td>
	<td>AN:<br>
	<TMPL_LOOP NAME=MSG_TO_LOOP>
     		<p><input type=hidden name="MSG_TO_UID" value="<TMPL_VAR NAME=MSG_TO_UID>"><TMPL_VAR NAME=MSG_TO_NAME></p>
	</TMPL_LOOP>

	</td>
</tr>
<tr>
	<td>
	<b>Subject:</b>
	</td>
	<td>
	<TMPL_IF NO_SUBJECT>
		<font color="#ff0000"><TMPL_VAR NO_SUBJECT></font>
	</TMPL_IF>
	<br>
	<TMPL_VAR SUBJECT>
